{
  "collectCoverage": true,
  "moduleFileExtensions": ["js", "mjs"],
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.mjs$": "babel-jest"
  },
  "reporters": [
    "default",
    [
      "jest-html-reporters",
      {
        "publicPath": "./tests/results",
        "filename": "report.html",
        "expand": true
      }
    ]   
  ],

  "testRegex": "((\\.|/*.)(spec))\\.js?$"
}
